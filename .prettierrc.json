{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 100,
  "trailingComma": "none",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "indent": 2,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
